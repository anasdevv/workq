{
  "name": "@workq/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx run-many -t serve -p workq-auth workq-jobs workq-executor",
    "prepare": "husky",
    "generate-ts-proto": "protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=./types ./proto/*.proto --ts_proto_opt=nestJs=true"
  },
  "nx": {
    "targets": {
      "generate-ts-proto": {
        "inputs": [
          "{workspaceRoot}/proto/*.proto"
        ],
        "cache": true
      }
    }
  },
  "private": true,
